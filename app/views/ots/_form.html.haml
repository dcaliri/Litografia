= form_for @ot do |f|
  -if @ot.errors.any?
    #error_explanation
      %h2= "#{pluralize(@ot.errors.count, "error")} prohibited this ot from being saved:"
      %ul
        - @ot.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :ot
    = f.text_field :ot
  .field
    = f.label :estado
    = f.text_field :estado
  .actions
    = f.submit 'Save'
