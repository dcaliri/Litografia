= form_for @cliente do |f|
  -if @cliente.errors.any?
    #error_explanation
      %h2= "#{pluralize(@cliente.errors.count, "error")} prohibited this cliente from being saved:"
      %ul
        - @cliente.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :cliente
    = f.text_field :cliente
  .field
    = f.label :procentajeMaximo
    = f.number_field :procentajeMaximo
  .actions
    = f.submit 'Save'
